    "type": "git",
    "url": "https://github.com/APIDevTools/swagger-parser.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf .nyc_output coverage",
    "lint": "eslint lib test",
