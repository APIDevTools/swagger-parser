    "chai": "^5",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.5",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsdoc": "^50.7.1",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.2.0",
    "js-yaml": "^4.1.0",
    "mocha": "^11.5.0",
    "nyc": "^17.1.0",
    "openapi-types": "^12.1.3",
