{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run tests",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "stopOnEntry": false,
      "args": [
        "--quick-test",
        "--timeout=600000"
      ],
      "cwd": "${workspaceRoot}",
      "preLaunchTask": null,
      "runtimeExecutable": null,
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "development",
        "NODE_TLS_REJECT_UNAUTHORIZED": "0"
      },
      "console": "internalConsole",
      "sourceMaps": false,
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
